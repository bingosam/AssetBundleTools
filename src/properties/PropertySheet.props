    <BuildMacro Include="DepDir">
      <Value>$(DepDir)</Value>
    </BuildMacro>
    <BuildMacro Include="UabeVer">
      <Value>$(UabeVer)</Value>
    </BuildMacro>
    <BuildMacro Include="UabeDir">
      <Value>$(UabeDir)</Value>
    </BuildMacro>
    <BuildMacro Include="UabeLibDir">
      <Value>$(UabeLibDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>